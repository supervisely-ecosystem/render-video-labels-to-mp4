{
  "name": "Render Video Labels to MP4",
  "type": "app",
  "categories": [
    "videos",
    "presentation"
  ],
  "description": "Creates presentation mp4 file based on labeled video",
  "docker_image": "supervisely/base-py-sdk:6",
  "main_script": "src/main.py",
  "modal_template": "src/modal.html",
  "modal_template_state": {
    "videoId": "",
    "allFrames": true,
    "startFrame": 0,
    "endFrame": 0,
    "showClassName": true,
    "thickness": 3,
    "opacity": 50
  },
  "task_location": "workspace_tasks",
  "isolate": true,
  "headless": true,
  "icon": "https://i.imgur.com/JH93zul.png",
  "icon_background": "#FFFFFF",
  "poster": "https://i.imgur.com/4hmKT5z.png"
}